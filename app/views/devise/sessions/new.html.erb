<h2>Entrar no Itaboraí Rotativo</h2>

<%= simple_form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-inputs col-md-9">
    <%= f.input :email, required: false, autofocus: true %>
    <%= f.input :password, label: "Senha", required: false %>
    <%= f.input :remember_me, label: "Lembrar Informações", as: :boolean if devise_mapping.rememberable? %>
    <%= f.button :submit, "Entrar" %><br />
    <%= render "devise/shared/links" %>
  </div>
<% end %>
